@model IEnumerable<BeCochera>
@{Layout = null;}
@if (Model.Count() > 0)
 {
<div class='row'>
<div class='col-md-12 col-sm-12'>
<table id='tblCochera' class='table table-bordered mb-0'>
<thead class='thead-default'>
<tr>
</tr>
</thead>
<tbody>
@foreach (BeCochera item in Model) {
<tr>
<td>
 <button type='button' title='Editar' class='btn btn-icon btn-pure success edita-cochera' data-target='#WinComun' data-codigo='{ "CoCochera": @item.CoCochera,  "CoEstadoPagina": "@Enu.CoEstadoPagina.Edicion"}'><i class='fa fa-pencil'></i></button></td>
<td>
<button type='button' data-codigo='{"CoCochera": @item.CoCochera, "CO_EST_REG": @(item.CO_EST_REG == 1 ? 0 : 1)  }' title=@(item.CO_EST_REG == 1 ? "¿Desactivar?": "¿Activar ?") class='btn btn-icon btn-pure success' data-toggle='activa-cochera' data-placement='top'>
<i class='@(item.CO_EST_REG == 1 ? "fa fa-check" : "fa fa-thumbs-o-down")'></i>
</button>
</td>
<td>
<button type='button' data-codigo='{"CoCochera": @item.CoCochera}' title='¿Eliminar?' class='btn btn-icon btn-pure danger' data-toggle='elimina-cochera' data-placement='top'>
<i class='fa fa-trash-o'></i>
</button>
</td>
<td>
@item.CoCondominio</td>
<td>
@item.CoEdificio</td>
<td>
@item.NuCochera</td>
<td>
@item.NuArea</td>
<td>
@item.NuPiso</td>
</tr>
}
</tbody>
</table>
</div>
</div>
}
@Glo.Paginador()
