@model BeServicioCalculo
@{ Layout = null;}
<form class='form form-horizontal form-bordered' id ='frmServiciocalculo' >
<div class='form-body'>
@Html.hidden(x => x.CoServicioCalculo)
<div class='form-group row'>
<label class='col-md-3 label-control'  for='CoEmpresa' >Empresa</label><div class="col-md-9">
@Html.number(x => x.CoEmpresa, new { @class = "form-control" }, null, false, true)
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='CoServicio' >Servicio</label><div class="col-md-9">
@Html.number(x => x.CoServicio, new { @class = "form-control" }, null, false, true)
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='CoTipoPredio' >Tipopredio</label><div class="col-md-9">
@Html.number(x => x.CoTipoPredio, new { @class = "form-control" }, null, false, true)
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='CoTipoCalculo' >Tipocalculo</label><div class="col-md-9">
@Html.number(x => x.CoTipoCalculo, new { @class = "form-control" }, null, false, true)
</div>
</div>
</div>
@Html.hidden(x => x.CoEstadoPagina)
</form>
<script type="text/javascript">
$(function () {
$('#frmServiciocalculo').validate(DefaultOptionValidate);
$('#WinComun button[id=btnGraba]').unbind('click').on('click', GrabaServiciocalculo);
 });

GrabaServiciocalculo = function () {
if ($('#frmServiciocalculo').valid()) {
var data = FormToJson(document.getElementById('frmServiciocalculo'));
$.ajax({
url: '@Url.Action("Graba","api/ServiciocalculoApi", new { area= "Condominio" })',
dataType: 'json',
type: 'POST',
cache: false,
data: data,
success: function (d) {
if (d.success == true) {
var item = d.data;
GetServiciocalculo.call({ 'codigo': item });
GetsServiciocalculo();
}
}
 });
}
}
</script>
