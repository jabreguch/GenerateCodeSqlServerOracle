@model IEnumerable<BeServicioCalculo>
@{Layout = null;}
@if (Model.Count() > 0)
 {
<div class='row'>
<div class='col-md-12 col-sm-12'>
<table id='tblServiciocalculo' class='table table-bordered mb-0'>
<thead class='thead-default'>
<tr>
</tr>
</thead>
<tbody>
@foreach (BeServicioCalculo item in Model) {
<tr>
<td>
 <button type='button' title='Editar' class='btn btn-icon btn-pure success edita-serviciocalculo' data-target='#WinComun' data-codigo='{ "CoServicioCalculo": @item.CoServicioCalculo,  "CoEstadoPagina": "@Enu.CoEstadoPagina.Edicion"}'><i class='fa fa-pencil'></i></button></td>
<td>
<button type='button' data-codigo='{"CoServicioCalculo": @item.CoServicioCalculo, "CO_EST_REG": @(item.CO_EST_REG == 1 ? 0 : 1)  }' title=@(item.CO_EST_REG == 1 ? "¿Desactivar?": "¿Activar ?") class='btn btn-icon btn-pure success' data-toggle='activa-serviciocalculo' data-placement='top'>
<i class='@(item.CO_EST_REG == 1 ? "fa fa-check" : "fa fa-thumbs-o-down")'></i>
</button>
</td>
<td>
<button type='button' data-codigo='{"CoServicioCalculo": @item.CoServicioCalculo}' title='¿Eliminar?' class='btn btn-icon btn-pure danger' data-toggle='elimina-serviciocalculo' data-placement='top'>
<i class='fa fa-trash-o'></i>
</button>
</td>
<td>
@item.CoEmpresa</td>
<td>
@item.CoServicio</td>
<td>
@item.CoTipoPredio</td>
<td>
@item.CoTipoCalculo</td>
</tr>
}
</tbody>
</table>
</div>
</div>
}
@Glo.Paginador()
