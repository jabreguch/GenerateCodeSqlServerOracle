@model IEnumerable<BeApartamentoInquilino>
@{Layout = null;}
@if (Model.Count() > 0)
 {
<div class='row'>
<div class='col-md-12 col-sm-12'>
<table id='tblApartamentoinquilino' class='table table-bordered mb-0'>
<thead class='thead-default'>
<tr>
</tr>
</thead>
<tbody>
@foreach (BeApartamentoInquilino item in Model) {
<tr>
<td>
 <button type='button' title='Editar' class='btn btn-icon btn-pure success edita-apartamentoinquilino' data-target='#WinComun' data-codigo='{ "CoCondominio": @item.CoCondominio,  "CoEstadoPagina": "@Enu.CoEstadoPagina.Edicion"}'><i class='fa fa-pencil'></i></button></td>
<td>
<button type='button' data-codigo='{"CoCondominio": @item.CoCondominio, "CO_EST_REG": @(item.CO_EST_REG == 1 ? 0 : 1)  }' title=@(item.CO_EST_REG == 1 ? "¿Desactivar?": "¿Activar ?") class='btn btn-icon btn-pure success' data-toggle='activa-apartamentoinquilino' data-placement='top'>
<i class='@(item.CO_EST_REG == 1 ? "fa fa-check" : "fa fa-thumbs-o-down")'></i>
</button>
</td>
<td>
<button type='button' data-codigo='{"CoCondominio": @item.CoCondominio}' title='¿Eliminar?' class='btn btn-icon btn-pure danger' data-toggle='elimina-apartamentoinquilino' data-placement='top'>
<i class='fa fa-trash-o'></i>
</button>
</td>
<td>
 <button type='button' title='Editar' class='btn btn-icon btn-pure success edita-apartamentoinquilino' data-target='#WinComun' data-codigo='{ "CoEdificio": @item.CoEdificio,  "CoEstadoPagina": "@Enu.CoEstadoPagina.Edicion"}'><i class='fa fa-pencil'></i></button></td>
<td>
<button type='button' data-codigo='{"CoEdificio": @item.CoEdificio, "CO_EST_REG": @(item.CO_EST_REG == 1 ? 0 : 1)  }' title=@(item.CO_EST_REG == 1 ? "¿Desactivar?": "¿Activar ?") class='btn btn-icon btn-pure success' data-toggle='activa-apartamentoinquilino' data-placement='top'>
<i class='@(item.CO_EST_REG == 1 ? "fa fa-check" : "fa fa-thumbs-o-down")'></i>
</button>
</td>
<td>
<button type='button' data-codigo='{"CoEdificio": @item.CoEdificio}' title='¿Eliminar?' class='btn btn-icon btn-pure danger' data-toggle='elimina-apartamentoinquilino' data-placement='top'>
<i class='fa fa-trash-o'></i>
</button>
</td>
<td>
 <button type='button' title='Editar' class='btn btn-icon btn-pure success edita-apartamentoinquilino' data-target='#WinComun' data-codigo='{ "CoApartamento": @item.CoApartamento,  "CoEstadoPagina": "@Enu.CoEstadoPagina.Edicion"}'><i class='fa fa-pencil'></i></button></td>
<td>
<button type='button' data-codigo='{"CoApartamento": @item.CoApartamento, "CO_EST_REG": @(item.CO_EST_REG == 1 ? 0 : 1)  }' title=@(item.CO_EST_REG == 1 ? "¿Desactivar?": "¿Activar ?") class='btn btn-icon btn-pure success' data-toggle='activa-apartamentoinquilino' data-placement='top'>
<i class='@(item.CO_EST_REG == 1 ? "fa fa-check" : "fa fa-thumbs-o-down")'></i>
</button>
</td>
<td>
<button type='button' data-codigo='{"CoApartamento": @item.CoApartamento}' title='¿Eliminar?' class='btn btn-icon btn-pure danger' data-toggle='elimina-apartamentoinquilino' data-placement='top'>
<i class='fa fa-trash-o'></i>
</button>
</td>
<td>
 <button type='button' title='Editar' class='btn btn-icon btn-pure success edita-apartamentoinquilino' data-target='#WinComun' data-codigo='{ "CoInquilino": @item.CoInquilino,  "CoEstadoPagina": "@Enu.CoEstadoPagina.Edicion"}'><i class='fa fa-pencil'></i></button></td>
<td>
<button type='button' data-codigo='{"CoInquilino": @item.CoInquilino, "CO_EST_REG": @(item.CO_EST_REG == 1 ? 0 : 1)  }' title=@(item.CO_EST_REG == 1 ? "¿Desactivar?": "¿Activar ?") class='btn btn-icon btn-pure success' data-toggle='activa-apartamentoinquilino' data-placement='top'>
<i class='@(item.CO_EST_REG == 1 ? "fa fa-check" : "fa fa-thumbs-o-down")'></i>
</button>
</td>
<td>
<button type='button' data-codigo='{"CoInquilino": @item.CoInquilino}' title='¿Eliminar?' class='btn btn-icon btn-pure danger' data-toggle='elimina-apartamentoinquilino' data-placement='top'>
<i class='fa fa-trash-o'></i>
</button>
</td>
<td>
@item.FeIniContrato</td>
<td>
@item.FeFinContrato</td>
<td>
@item.FeContrato</td>
<td>
@item.BlContrato</td>
<td>
@item.NoArchivo</td>
<td>
@item.NoArchivoExtencion</td>
</tr>
}
</tbody>
</table>
</div>
</div>
}
@Glo.Paginador()
