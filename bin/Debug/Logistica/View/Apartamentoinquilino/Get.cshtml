@model BeApartamentoInquilino
@{ Layout = null;}
<form class='form form-horizontal form-bordered' id ='frmApartamentoinquilino' >
<div class='form-body'>
@Html.hidden(x => x.CoCondominio)
@Html.hidden(x => x.CoEdificio)
@Html.hidden(x => x.CoApartamento)
@Html.hidden(x => x.CoInquilino)
<div class='form-group row'>
<label class='col-md-3 label-control'  for='FeIniContrato' >Inicontrato</label><div class="col-md-9">
@Html.text(x => x.FeIniContrato, new { @class = "form-control", @maxlength = "8" }, null, false, true)
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='FeFinContrato' >Fincontrato</label><div class="col-md-9">
@Html.text(x => x.FeFinContrato, new { @class = "form-control", @maxlength = "8" }, null, false, true)
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='FeContrato' >Contrato</label><div class="col-md-9">
@Html.text(x => x.FeContrato, new { @class = "form-control", @maxlength = "8" }, null, false, true)
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='BlContrato' >Contrato</label><div class="col-md-9">
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='NoArchivo' >Archivo</label><div class="col-md-9">
@Html.textarea(x => x.NoArchivo, new { @class = "form-control", @maxlength = "250", @rows = "6" })
</div>
</div>
<div class='form-group row'>
<label class='col-md-3 label-control'  for='NoArchivoExtencion' >Archivoextencion</label><div class="col-md-9">
@Html.text(x => x.NoArchivoExtencion, new { @class = "form-control", @maxlength = "5" }, null, false, true)
</div>
</div>
</div>
@Html.hidden(x => x.CoEstadoPagina)
</form>
<script type="text/javascript">
$(function () {
$('#frmApartamentoinquilino').validate(DefaultOptionValidate);
$('#WinComun button[id=btnGraba]').unbind('click').on('click', GrabaApartamentoinquilino);
 });

GrabaApartamentoinquilino = function () {
if ($('#frmApartamentoinquilino').valid()) {
var data = FormToJson(document.getElementById('frmApartamentoinquilino'));
$.ajax({
url: '@Url.Action("Graba","api/ApartamentoinquilinoApi", new { area= "Condominio" })',
dataType: 'json',
type: 'POST',
cache: false,
data: data,
success: function (d) {
if (d.success == true) {
var item = d.data;
GetApartamentoinquilino.call({ 'codigo': item });
GetsApartamentoinquilino();
}
}
 });
}
}
</script>
